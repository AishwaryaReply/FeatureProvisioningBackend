{
    "$schema": "http://json-schema.org/draft-07/schema",
    "description": "schema to validate create feature",
    "type": "object",
    "properties": {
        "headers": {
            "type": "object",
            "properties": {
                
                "x-device-id": {
                    "type": "string"
                },
                "x-originator-type": {
                    "type": "string"
                },
                "clientrequestid": {
                    "type": "string"
                },
                "x-clientapp-name": {
                    "type": "string"
                },
                "x-clientapp-version": {
                    "type": "string"
                },
                "locale": {
                    "type": "string"
                },
                "content-type": {
                    "type": "string",
                    "pattern": "^(application\/json)(.*)?$"
                }
            },
            "required": [
                "clientrequestid",
                "x-originator-type",
				"dealer-authorization",
				"x-clientapp-version",
				"content-type"
            ]
        },
        "body": {
            "type":"object",
            "properties": {
                "code": {
                    "type": "string"
                },                
                "description": {
                    "type": "string"
                },
                "channel": {
                    "type": "string"
                }
            }
        }
    },
    "required": [
        "headers",
        "queryString"
    ]
}