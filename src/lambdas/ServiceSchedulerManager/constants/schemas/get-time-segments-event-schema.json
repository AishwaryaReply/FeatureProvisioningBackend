{
    "$schema": "http://json-schema.org/draft-07/schema",
    "description": "schema to validate the get time segments event",
    "type": "object",
    "properties": {
        "headers": {
            "type": "object",
            "properties": {
                "x-device-id": {
                    "type": "string"
                },
                "x-originator-type": {
                    "type": "string"
                },
                "clientrequestid": {
                    "type": "string"
                },
                "x-clientapp-name": {
                    "type": "string"
                },
                "x-clientapp-version": {
                    "type": "string"
                },
                "locale": {
                    "type": "string"
                },
				"dealer-authorization": {
                    "type": "string"
				},
                "content-type": {
                    "type": "string",
                    "pattern": "^(application\/json)(.*)?$"
                }
            },
            "required": [
                "clientrequestid",
                "x-originator-type",
				"dealer-authorization",
				"x-clientapp-version",
				"content-type"
            ]
        },
        "pathParams": {
            "type": "object",
            "properties": {
                "departmentId": {
                    "type": "string"
                }
            },
            "required": [
                "departmentId"
            ]
        },
        "queryString": {
            "type": "object",
            "properties": {
                "startDate": {
                    "type": "string"
                },
                "endDate": {
                    "type": "string"
                }
            },
            "required": [
                "startDate",
				"endDate"
            ]
        }
    },
    "required": [
        "headers",
        "pathParams",
		"queryString"
    ]
}