{
    "$schema": "http://json-schema.org/draft-07/schema",
    "description": "schema to validate the get publication event",
    "type": "object",
    "properties": {
        "headers": {
            "type": "object",
            "properties": {
                "x-device-id": {
                    "type": "string"
                },
                "x-originator-type": {
                    "type": "string"
                },
                "clientrequestid": {
                    "type": "string"
                },
                "x-clientapp-name": {
                    "type": "string"
                },
                "x-clientapp-version": {
                    "type": "string"
                },
                "locale": {
                    "type": "string"
                },
                "content-type": {
                    "type": "string",
                    "pattern": "^(application\/json)(.*)?$"
                }
            },
            "required": [
                "clientrequestid",
                "x-originator-type"
            ]
        },
        "pathParams": {
            "type": "object",
            "properties": {
                "vin": {
                    "type": "string"
                },
                "userid": {
                    "type": "string"
                }
            },
            "required": [
                "vin",
                "userid"
            ]
        },
        "queryString": {
            "type": "object",
            "properties": {
                "odometer": {
                    "type": "string"
                }
            },
            "required": [
                "odometer"
            ]
        }
    },
    "required": [
        "headers",
        "queryString",
        "pathParams"
    ]
}