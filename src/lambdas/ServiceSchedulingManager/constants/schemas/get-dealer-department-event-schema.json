{
    "$schema": "http://json-schema.org/draft-07/schema",
    "description": "schema to validate the get dealer department event",
    "definitions": {
        "Service": {
            "type": "object",
            "properties": {
                "id": {
                    "type": "string"
                }
            },
            "required": [
                "id"
            ]
        }
    },
    "type": "object",
    "properties": {
        "headers": {
            "type": "object",
            "properties": {
                "x-device-id": {
                    "type": "string"
                },
                "x-originator-type": {
                    "type": "string"
                },
                "clientrequestid": {
                    "type": "string"
                },
                "x-clientapp-name": {
                    "type": "string"
                },
                "x-clientapp-version": {
                    "type": "string"
                },
                "locale": {
                    "type": "string"
                },
                "dealer-authorization": {
                    "type": "string"
                },
                "content-type": {
                    "type": "string",
                    "pattern": "^(application\/json)(.*)?$"
                }
            },
            "required": [
                "clientrequestid",
                "x-originator-type",
                "dealer-authorization",
                "x-clientapp-version",
                "content-type"
            ]
        },
        "requestBody": {
            "type": "object",
            "properties": {
                "servicesList": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/Service"
                    }
                }
            },
            "required": [
                "servicesList"
            ]
        }
    },
    "required": [
        "headers",
        "requestBody"
    ]
}